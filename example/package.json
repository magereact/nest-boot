{
  "private": true,
  "name": "example",
  "version": "2.2.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nest build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "nest-boot": "nest-boot"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.5.3",
    "@mikro-orm/nestjs": "^5.1.2",
    "@mikro-orm/postgresql": "^5.5.3",
    "@mikro-orm/reflection": "^5.5.3",
    "@nest-boot/auth": "3.2.1",
    "@nest-boot/command": "3.2.0",
    "@nest-boot/common": "3.2.0",
    "@nest-boot/database": "3.3.1",
    "@nest-boot/graphql": "3.2.1",
    "@nest-boot/health-check": "3.1.1",
    "@nest-boot/i18next": "3.1.1",
    "@nest-boot/mailer": "3.1.1",
    "@nest-boot/queue": "3.2.0",
    "@nest-boot/redis": "2.1.0",
    "@nest-boot/search": "3.3.1",
    "@nest-boot/search-engine-postgresql": "2.3.1",
    "@nest-boot/validator": "3.1.1",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/platform-express": "^9.2.0",
    "@node-rs/argon2": "^1.2.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "meilisearch": "^0.28.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "snowflake-id-generator": "^1.0.4",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.5.3",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/source-map-support": "^0.5.6",
    "@types/supertest": "^2.0.12",
    "jest": "^29.3.1",
    "pino-pretty": "^9.1.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "extends": "../package.json"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
